{
	"name": "ide-svelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "cross-env FIREBASE_AUTH_EMULATOR_HOST=\"127.0.0.1:9099\" concurrently -k \"firebase emulators:start\" \"vite dev\"",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:e2e": "playwright test",
		"test": "npm run test:e2e",
		"postinstall": "patch-package",
		"emulators:stop": "lsof -t -i:8080 -i:9000 -i:9099 -i:9199 -i:9090 | xargs kill -9"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.3",
		"@melt-ui/pp": "^0.3.2",
		"@melt-ui/svelte": "^0.86.2",
		"@playwright/test": "^1.45.3",
		"@sveltejs/adapter-vercel": "^5.5.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/vscode": "^1.96.0",
		"autoprefixer": "^10.4.20",
		"concurrently": "^9.1.2",
		"cross-env": "^7.0.3",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"patch-package": "^8.0.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.4.11"
	},
	"dependencies": {
		"@babel/runtime": "^7.26.0",
		"@codemirror/lang-cpp": "^6.0.2",
		"@codemirror/lang-java": "^6.0.1",
		"@codemirror/lang-python": "^6.1.7",
		"@codingame/esbuild-import-meta-url-plugin": "^1.0.2",
		"@codingame/monaco-vscode-configuration-service-override": "^11.1.2",
		"@codingame/monaco-vscode-cpp-default-extension": "^11.1.2",
		"@codingame/monaco-vscode-files-service-override": "^11.1.2",
		"@codingame/monaco-vscode-java-default-extension": "^11.1.2",
		"@codingame/monaco-vscode-keybindings-service-override": "^11.1.2",
		"@codingame/monaco-vscode-python-default-extension": "^11.1.2",
		"@codingame/monaco-vscode-rollup-vsix-plugin": "^11.1.2",
		"@codingame/monaco-vscode-storage-service-override": "^11.1.2",
		"@codingame/monaco-vscode-theme-defaults-default-extension": "^11.1.2",
		"@codingame/monaco-vscode-view-status-bar-service-override": "^11.1.2",
		"@replit/codemirror-vim": "^6.2.1",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.3",
		"@uiw/codemirror-theme-github": "^4.23.7",
		"@uiw/codemirror-theme-vscode": "^4.23.7",
		"codemirror": "^6.0.1",
