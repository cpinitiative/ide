		"firebase-admin": "^13.0.2",
		"lucide-svelte": "^0.471.0",
		"monaco-editor-wrapper": "^6.1.0",
		"split-grid": "^1.0.11",
		"y-codemirror.next": "^0.3.5",
		"y-monaco": "^0.1.6",
		"y-protocols": "^1.0.6",
		"y-websocket": "^2.1.0",
		"yjs": "^13.6.21"
	}
}
